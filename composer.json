{
    "name": "sandorl195/vatin",
    "description": "Validate VAT identification numbers",
    "keywords": [ "vat", "tax", "btw", "vies" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sándor László",
            "email": "sandorl195@gmail.com",
            "homepage": "https://github.com/sandorl195"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/sandorl195/vatin/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "ext-soap": "*",
        "phpunit/phpunit": "^5.7 || ^6.5"
    },
    "suggest": {
        "ext-soap": "Required if you want to check the VAT number via VIES"
    },
    "autoload": {
        "psr-4": {
            "Ddeboer\\Vatin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ddeboer\\Vatin\\Test\\": "tests/"
        }
    }
}
